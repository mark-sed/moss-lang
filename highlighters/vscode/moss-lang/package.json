{
  "name": "moss-lang",
  "displayName": "Moss Lang",
  "description": "Syntax highlighting for Moss language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "moss",
      "aliases": ["Moss", "moss"],
      "extensions": [".ms",".moss",".mss"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "moss",
      "scopeName": "source.moss",
      "path": "./syntaxes/moss.tmLanguage.json"
    }]
  }
}
