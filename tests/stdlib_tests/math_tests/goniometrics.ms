import math

// atan
math.atan(0)        // 0.0
"\n"
math.atan(1)        // 0.7853981633974483   (π / 4)
"\n"
math.atan(-1)       // -0.7853981633974483  (-π / 4)

"\n"
math.atan(0.5)      // 0.4636476090008061
"\n"
math.atan(2)        // 1.1071487177940904

"\n"
math.atan(10)       // 1.4711276743037347
"\n"
math.atan(-10)      // -1.4711276743037347

"\n"
math.atan(Float("inf")) == math.pi / 2   // (π / 2)
"\n"
math.atan(Float("-inf")) == -math.pi / 2 // (-π / 2)

"\n"
math.atan(1e-10) == 1e-10   // 1e-10 (approximately)
"\n"

// atan2
math.atan2(0, 1)        // 0.0
"\n"
math.atan2(1, 0)        // 1.5707963267948966   (π / 2)
"\n"
math.atan2(0, -1)       // 3.141592653589793    (π)
"\n"
math.atan2(-1, 0)       // -1.5707963267948966  (-π / 2)

"\n"
math.atan2(1, 1)        // 0.7853981633974483   (π / 4)
"\n"
math.atan2(-1, 1)       // -0.7853981633974483  (-π / 4)
"\n"
math.atan2(1, -1)       // 2.356194490192345    (3π / 4)
"\n"
math.atan2(-1, -1)      // -2.356194490192345   (-3π / 4)

"\n"
math.atan2(2, 3)        // 0.5880026035475675
"\n"
math.atan2(0, 0)        // 0.0
"\n"
math.atan2(1, math.inf) // 0.0
"\n"
math.atan2(1, -math.inf) == math.pi // 3.141592653589793    (π)
"\n"

math.sin(Complex(1, 2))    // (3.165778513216168+1.9596010414216063j)
"\n"
math.cos(Complex(1.0, 2.0))    // (2.0327230070196656-3.0518977991518j)
"\n"
math.tan(Complex(1, 2))    // (0.0338128260798967+1.0147936161466335j)
"\n"

math.sin(Complex(0, 1))    // 1.1752011936438014j
"\n"
math.cos(Complex(0, 1))    // (1.5430806348152437+0j)
"\n"
math.tan(Complex(0, 1))    // 0.7615941559557649j
"\n"

math.sin(Complex(3.0, 0.0))    // (0.1411200080598672+0j)
"\n"
math.cos(Complex(3, 0))    // (-0.9899924966004454+0j)
"\n"
math.tan(Complex(3, 0))    // (-0.1425465430742778+0j)
"\n"